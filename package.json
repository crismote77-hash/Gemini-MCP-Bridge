{
  "name": "gemini-mcp-bridge",
  "version": "0.1.0",
  "description": "MCP server exposing Gemini capabilities to AI CLIs",
  "main": "dist/index.js",
  "bin": {
    "gemini-mcp-bridge": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/prebuild.mjs",
    "build": "tsc",
    "postbuild": "node scripts/postbuild.mjs",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "runbook:note": "node scripts/runbook-note.mjs"
  },
  "keywords": ["mcp", "gemini", "google", "ai", "model-context-protocol", "codex"],
  "author": "Cristobal Mora <crismote77@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.4.0",
    "redis": "^5.10.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.53.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
